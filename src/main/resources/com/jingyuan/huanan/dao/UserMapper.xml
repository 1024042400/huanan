<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingyuan.huanan.dao.UserDao">
    <select id="selectUser" resultType="com.jingyuan.huanan.model.User">
        select * from g2048.juba_users where id = #{id} limit 1
    </select>
</mapper>
